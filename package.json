    "cibsdev": "node ./node_modules/klassi-js/index.js --disableReport --email --tags @search --remoteService browserstack --extraSettings",
    "ltlocal": "node ./node_modules/klassi-js/index.js --disableReport --remoteService lambdatest --extraSettings",
